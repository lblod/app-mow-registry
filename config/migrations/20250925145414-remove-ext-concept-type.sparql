PREFIX dct: <http://purl.org/dc/terms/>
PREFIX ext: <http://mu.semte.ch/vocabularies/ext/>

DELETE {
  GRAPH <http://mu.semte.ch/graphs/mow/registry> {
    ?sub 
      a ext:Concept;
      dct:modified ?oldModified.
  }
}
INSERT {
  GRAPH <http://mu.semte.ch/graphs/mow/registry> {
    ?sub dct:modified ?now.
  }
}
WHERE {
  GRAPH <http://mu.semte.ch/graphs/mow/registry> {
    ?sub a ext:Concept.
    OPTIONAL {
      ?sub dct:modified ?oldModified.
    }
  }
  BIND(NOW() AS ?now)
}