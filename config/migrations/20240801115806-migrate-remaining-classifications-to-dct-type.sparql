PREFIX org: <http://www.w3.org/ns/org#>
PREFIX mobiliteit: <https://data.vlaanderen.be/ns/mobiliteit#>
PREFIX dct: <http://purl.org/dc/terms/>

DELETE {
    GRAPH ?g {
        ?s org:classification ?o .
    }
}

INSERT{
    GRAPH ?g {
        ?s dct:type ?o .
    }
}
WHERE{
    GRAPH ?g {
        ?s a mobiliteit:Verkeerstekenconcept ;
           org:classification ?o .
    } 
}