{
  "version": "0.1",
  "prefixes": {
    "lblodmow": "http://data.lblod.info/vocabularies/mobiliteit/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "org": "http://www.w3.org/ns/org#",
    "mobiliteit": "https://data.vlaanderen.be/ns/mobiliteit#",
    "vs": "http://www.w3.org/2003/06/sw-vocab-status/ns#",
    "ext": "http://mu.semte.ch/vocabularies/ext/",
    "sh": "http://www.w3.org/ns/shacl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "qb": "http://purl.org/linked-data/cube#"
  },
  "resources": {
    "resources": {
      "name": "resource",
      "class": "rdfs:Resource",
      "new-resource-base": "http://data.lblod.info/Resources/",
      "relationships": {
        "used": {
          "predicate": "ext:used",
          "target": "concept",
          "cardinality": "many"
        }
      }
    },
    "shapes": {
      "name": "shape",
      "class": "sh:Shape",
      "relationships": {
        "targetClass": {
          "predicate": "sh:targetClass",
          "target": "resource",
          "cardinality": "one"
        },
        "targetNode": {
          "predicate": "sh:targetNode",
          "target": "resource",
          "cardinality": "one"
        },
        "targetHasConcept": {
          "predicate": "ext:targetHasConcept",
          "target": "concept",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.lblod.info/Shapes/"
    },
    "property-shapes": {
      "name": "propertyShape",
      "class": "sh:PropertyShape",
      "super": ["shape"],
      "relationships": {
        "path": {
          "predicate": "sh:path",
          "target": "resource",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.lblod.info/PropertyShapes/"
    },
    "node-shapes": {
      "name": "nodeShape",
      "class": "sh:NodeShape",
      "super": ["shape"],
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "new-resource-base": "http://data.lblod.info/NodeShapes/"
    },
    "concepts":{
      "name": "concept",
      "class": "ext:Concept",
      "super": ["resource"],
      "relationships": {
        "template": {
          "predicate": "ext:template",
          "target": "template",
          "cardinality": "one"
        },
        "relations": {
          "predicate": "ext:relation",
          "target": "relation",
          "cardinality": "many"
        }
      },
      "new-resource-base": "http://data.lblod.info/Concepts/"
    },
    "templates": {
      "name": "template",
      "class": "ext:Template",
      "attributes": {
        "value": {
          "type": "string",
          "predicate": "ext:value"
        }
      },
      "relationships": {
        "mappings": {
          "predicate": "ext:mapping",
          "target": "mapping",
          "cardinality": "many"
        }
      },
      "new-resource-base": "http://data.lblod.info/Templates/"
    },
    "mappings": {
      "name": "mapping",
      "class": "ext:Mapping",
      "attributes": {
        "variable": {
          "type": "string",
          "predicate": "ext:variable"
        },
        "type": {
          "type": "string",
          "predicate": "ext:variableType"
        }
      },
      "relationships": {
        "expects": {
          "predicate": "ext:expects",
          "target": "shape",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.lblod.info/Mappings/"
    },
    "relations": {
      "name": "relation",
      "class": "ext:Relation",
      "attributes": {
        "expectedNumber": {
          "type": "integer",
          "predicate": "ext:expectedNumber"
        },
        "reason": {
          "type": "string",
          "predicate": "ext:reason"
        },
        "order": {
          "type": "integer",
          "predicate": "qb:order"
        }
      },
      "relationships": {
        "concept": {
          "predicate": "ext:concept",
          "target": "concept",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.lblod.info/Relations/"
    },
    "can-be-combined-with-relations": {
      "name": "canBeCombinedWithRelation",
      "class": "ext:CanBeCombinedWithRelation",
      "super": ["relation"],
      "new-resource-base": "http://data.lblod.info/CanBeCombinedWithRelations/"
    },
    "must-use-relations": {
      "name": "mustUseRelation",
      "class": "ext:MustUseRelation",
      "super": ["relation"],
      "new-resource-base": "http://data.lblod.info/MustUseRelations/"
    },
    "road-sign-concepts": {
      "name": "roadSignConcept",
      "class": "mobiliteit:Verkeersbordconcept",
      "super": ["concept"],
      "attributes": {
        "image": {
          "type": "string",
          "predicate": "mobiliteit:grafischeWeergave"
        },
        "meaning": {
          "type": "string",
          "predicate": "skos:scopeNote"
        },
        "road-sign-concept-code": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "relationships": {
        "status": {
          "predicate": "vs:term_status",
          "target": "roadSignConceptStatus",
          "cardinality": "one"
        },
        "categories": {
          "predicate": "org:classification",
          "target": "roadSignCategory",
          "cardinality": "many"
        },
        "measures": {
          "predicate": "ext:measures",
          "target": "roadMeasure",
          "cardinality": "many"
        },
        "subSigns": {
          "predicate": "lblodmow:heeftOnderbordConcept",
          "target": "roadSignConcept",
          "cardinality": "many"
        },
        "mainSigns": {
          "predicate": "lblodmow:heeftOnderbordConcept",
          "target": "roadSignConcept",
          "cardinality": "many",
          "inverse": true
        },
        "relatedRoadSignConcepts": {
          "predicate": "lblodmow:heeftGerelateerdVerkeersbordconcept",
          "target": "roadSignConcept",
          "cardinality": "many"
        }
      },
      "new-resource-base": "http://data.lblod.info/verkeersbordconcepten/"
    },
    "road-marking-concepts": {
      "name": "roadMarkingConcept",
      "class": "mobiliteit:Wegmarkeringconcept",
      "super": ["concept"],
      "attributes": {
        "image": {
          "type": "string",
          "predicate": "mobiliteit:grafischeWeergave"
        },
        "meaning": {
          "type": "string",
          "predicate": "skos:scopeNote"
        },
        "definition": {
          "type": "string",
          "predicate": "skos:definition"
        },
        "road-marking-concept-code": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "relationships": {
        "status": {
          "predicate": "vs:term_status",
          "target": "roadMarkingConceptStatus",
          "cardinality": "one"
        },
        "relatedRoadSignConcepts": {
          "predicate": "lblodmow:heeftGerelateerdVerkeersbordconcept",
          "target": "roadSignConcept",
          "cardinality": "many"
        },
        "relatedRoadMarkingConcepts": {
          "predicate": "lblodmow:heeftGerelateerdWegmarkeringconcept",
          "target": "roadMarkingConcept",
          "cardinality": "many"
        }
      },
      "new-resource-base": "http://data.lblod.info/Wegmarkeringconcepten/"
    },
    "traffic-light-concepts": {
      "name": "trafficLightConcept",
      "class": "mobiliteit:Verkeerslichtconcept",
      "super": ["concept"],
      "attributes": {
        "image": {
          "type": "string",
          "predicate": "mobiliteit:grafischeWeergave"
        },
        "meaning": {
          "type": "string",
          "predicate": "skos:scopeNote"
        },
        "definition": {
          "type": "string",
          "predicate": "skos:definition"
        },
        "traffic-light-concept-code": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "relationships": {
        "status": {
          "predicate": "vs:term_status",
          "target": "trafficLightConceptStatus",
          "cardinality": "one"
        },
        "categories": {
          "predicate": "org:classification",
          "target": "trafficLightCategory",
          "cardinality": "many"
        },
        "relatedTrafficLightConcepts": {
          "predicate": "lblodmow:heeftGerelateerdVerkeerslichtconcept",
          "target": "trafficLightConcept",
          "cardinality": "many"
        }
      },
      "new-resource-base": "http://data.lblod.info/Verkeerslichtconcepten/"
    },
    "road-sign-categories": {
      "name": "roadSignCategory",
      "class": "mobiliteit:Verkeersbordcategorie",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "relationships": {
        "roadSignConcepts": {
          "predicate": "org:classification",
          "target": "roadSignConcept",
          "cardinality": "many",
          "inverse": true
        }
      },
      "new-resource-base": "http://data.lblod.info/verkeersbordcategorieen/"
    },
    "road-sign-concept-status": {
      "name": "roadSignConceptStatus",
      "class": "mobiliteit:VerkeersbordconceptStatus",
      "relationships": {
        "roadSignConceptStatusCode": {
          "predicate": "mobiliteit:VerkeersbordconceptStatus.status",
          "target": "roadSignConceptStatusCode",
          "cardinality": "one"
        },
        "roadSignConcept": {
          "predicate": "vs:term_status",
          "target": "roadSignConcept",
          "cardinality": "many",
          "inverse": true
        }
      },
      "new-resource-base": "http://data.lblod.info/verkeersbordconcept-status/"
    },
    "road-sign-concept-status-codes": {
      "name": "roadSignConceptStatusCode",
      "class": "lblodmow:VerkeersbordconceptStatusCode",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "relationships": {
        "roadSignConceptStatus": {
          "predicate": "mobiliteit:VerkeersbordconceptStatus.status",
          "target": "roadSignConceptStatus",
          "cardinality": "many",
          "inverse": true
        }
      },
      "new-resource-base": "http://data.lblod.info/verkeersbordconcept-status-codes/"
    },
    "indicator-types": {
      "name": "indicatorType",
      "class": "ext:IndicatorType",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        },
        "model-name": {
          "type": "string",
          "predicate": "ext:modelName"
        },
        "search-filter": {
          "type": "string",
          "predicate": "ext:searchFilter"
        },
        "sorting-field": {
          "type": "string",
          "predicate": "ext:sortingField"
        },
        "label-field": {
          "type": "string",
          "predicate": "ext:labelField"
        }
      },
      "new-resource-base": "http://lblod.data.gift/concepts/"
    }
  }
}
